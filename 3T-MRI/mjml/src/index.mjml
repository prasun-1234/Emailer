<mjml>
  <mj-head>
    <mj-preview>Costa mesa </mj-preview>
    <mj-breakpoint width="600px" />
    <mj-font name="Helvetica" href="https://fonts.cdnfonts.com/css/helvetica-neue-55" />
    <mj-attributes>
      <mj-all color="#033D5A" font-family="'Helvetica Neue, sans-serif'" padding="0px" />
      <mj-class name="border-radius" border-radius="8px" />
      <mj-class name="top-border" border-radius="4px 4px 0 0" />
      <mj-class name="btn-border" border-radius="40px" />
      <mj-class name="img-border" border-radius="16px" />
      <mj-class name="border" border="1px solid red" />
    </mj-attributes>


    <mj-style inline="inline">
      mj-button {
        padding: 0;
      }

      @media (prefers-color-scheme: dark) {
        .dark-mode-bg {
          background-color:
            #000000 !important;
        }

        .dark-mode-text {
          color: #FFFFFF !important;
        }

        .dark-mode-btn {
          background-color: #444444 !important;
          color: #FFFFFF !important;
        }

        .dark-mode-border {
          border-color: #FFFFFF !important;
        }
      }

      @media only screen and (max-width: 600px) {
        .responsive-wrapper {
          padding: 20px !important;
        }

        mj-wrapper {
          padding: 20px !important;
        }
      }
    </mj-style>

  </mj-head>
  <mj-body background-color="#CEF3FF">
    <!-- header hero -->
    <mj-wrapper background-color="#033D5A" mj-class="top-border">
      <!-- header logo box -->
      <mj-hero mode="fluid-height" background-width="600px" background-height="300px" background-url="https://clientstaging.space/emailer-images/3t-hero-bg.jpg" background-color="#033D5A" padding="34px 0">
        <mj-image src="https://clientstaging.space/emailer-images/3t-exp-logo.png" alt="logo" width="275px" />
        <mj-text padding="25px 0 0" color="#ffffff" font-family="Helvetica" align="center" font-style="italic" font-size="32px" line-height="39px" font-weight="700">
          New High-Resolution
        </mj-text>
        <mj-image src="https://clientstaging.space/emailer-images/3t-text.png" alt="text" width="600px" />
        <mj-button href="https://expertmri.com/locations/costa-mesa/" width="235px" align="center" background-color="transparent" font-size="16px" font-weight="700" font-family="Helvetica,sans-serif" line-height="180%" color="#ffffff" border-radius="40px" border="1px solid #fff" padding="10px 40px">
          SCHEDULE AN MRI
        </mj-button>
      </mj-hero>

      <!-- header img box -->
      <mj-section background-color="#033D5A" padding="0 ">
        <mj-column>
          <mj-image src="https://clientstaging.space/emailer-images/3t-bg-mri-machine.jpg" alt="" />
        </mj-column>
      </mj-section>
    </mj-wrapper>

    <!-- Rating badge section -->
    <mj-wrapper padding="40px" background-color="#ffffff" css-class="responsive-wrapper">
      <mj-section>
        <mj-column>
          <mj-text align="center" font-size="32px" font-weight="700" font-family="Helvetica,sans-serif" line-height="140%" css-class="dark-mode-text">
            Precision and Clarity Redefined: Unlock Superior Imaging with ExpertMRI's Cutting-Edge 3T MRI Services!
          </mj-text>
          <mj-image src="https://clientstaging.space/emailer-images/3t-rating-badge.png" alt="rating badge" width="600px" />
          <mj-text align="center" font-size="18px" font-weight="400" font-family="Helvetica,sans-serif" line-height="180%" css-class="dark-mode-text" padding="25px 0 0">
            Discover the ultimate in diagnostic imaging with ExpertMRI's cutting-edge 3T MRI services. Our advanced technology offers exceptional precision and clarity, providing you with the most accurate and detailed results. Choose ExpertMRI for superior imaging that redefines your healthcare experience.
          </mj-text>

        </mj-column>
      </mj-section>
    </mj-wrapper>

    <!-- image quality section -->
    <mj-section padding="0 0 40px" background-color="#EBEED7">
      <mj-column>
        <mj-image src="https://clientstaging.space/emailer-images/3t-full-banner.jpg" alt="doctore with mri report" />

        <mj-text align="center" font-size="32px" font-weight="700" line-height="140%" font-family="Helvetica, Sans serif" color="#033D5A" padding="24px 0 0">
          Unmatched Image Quality
        </mj-text>
        <mj-text align="center" font-size="18px" font-weight="400" line-height="180%" font-family="Helvetica, Sans serif" color="#033D5A" padding="0  32px">
          Experience unparalleled clarity with our advanced 3T MRI. Our state-of-the-art technology delivers superior image quality, enabling precise diagnoses and effective treatment plans. Trust our expertise for exceptional imaging results and personalized care
        </mj-text>

        <mj-button href="https://expertmri.com/locations/costa-mesa/" width="235px" padding="32px 0 0" align="center" background-color="#284FD8" font-size="16px" font-weight="700" font-family="Helvetica,sans-serif" line-height="180%" color="#ffffff" mj-class="btn-border">
          SCHEDULE AN MRI
        </mj-button>
      </mj-column>
    </mj-section>

